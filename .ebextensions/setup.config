container_commands:
pip_upgrade:
    command: "source /opt/python/run/venv/bin/activate && pip install --upgrade pip"
    ignoreErrors: false
migrate:
    command: "django-admin.py migrate"
    leader_only: true
collectstatic:
    command: "django-admin.py collectstatic --noinput"
wsgipass:
    command: 'echo "WSGIPassAuthorization On" >> ../wsgi.conf'